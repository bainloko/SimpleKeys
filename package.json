    "Backup"
  ],
  "author": "Kau√£ Maia (bainloko)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bainloko/SimpleKeys/issues"
  },
  "homepage": "https://github.com/bainloko/SimpleKeys#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "better-sqlite3-multiple-ciphers": "^7.6.2",
    "csv": "^6.2.0",
    "electron": "^19.0.10",
    "electron-better-ipc": "^2.0.1",
    "electron-json-storage": "^4.5.0",
    "electron-log": "^4.4.8",
    "electron-settings": "^4.0.2",
    "electron-unhandled": "^4.0.1",
    "fs-extra": "^10.1.0",
    "secure-electron-context-menu": "^1.3.3",
    "sequelize": "^6.21.0",
    "update-electron-app": "^2.0.1"
  },
  "devDependencies": {
    "@doyensec/electronegativity": "^1.9.1",
    "@types/electron-json-storage": "^4.5.0",
    "@types/fs-extra": "^9.0.13",
    "electron-packager": "^15.5.1",
    "nodemon": "^2.0.18",
    "sequelize-cli": "^6.4.1"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "simplekeys"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "linux"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
