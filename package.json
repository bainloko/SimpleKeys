{
  "name": "simplekeys",
  "productName": "SimpleKeys",
  "version": "0.2.0",
  "description": "Um gerenciador de senhas leve, versátil e seguro.",
  "main": "src/main.js",
  "scripts": {
    "test": "npx electron .",
    "build": "npx electron-packager . --platform=win32 --arch=x64 --icon icon.ico --out out/ --overwrite --win32metadata.ProductName='SimpleKeys' --win32metadata.requested-execution-level='requireAdministrator'",
    "setup": "npx electron-installer-windows --src out/SimpleKeys-win32-x64/ --dest out/installer --animation animation.gif --icon icon.ico --iconNuget icon.png --noMsi true --remoteReleases https://github.com/bainloko/SimpleKeys/releases/latest --certificateFile cert.pfx --certificatePassword ..."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bainloko/SimpleKeys.git"
  },
